def factorial(n):
    if n < 0:
        return "Factorial is not defined for negative numbers."
    
    result = 1
    for i in range(1, n + 1):
        result *= i  # Multiply result by i each time
    return result

# Call the function with a sample number
num = 5
print(f"The factorial of {num} is: {factorial(num)}")
